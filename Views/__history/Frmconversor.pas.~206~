  TFConversor = class(TForm)
    OpenDialog1: TOpenDialog;
    Edt55: TLabeledEdit;
    Edt65: TLabeledEdit;
    EdtTerceiros: TLabeledEdit;
    EdtServicos: TLabeledEdit;
    Button1: TButton;
    Button2: TButton;
    Button3: TButton;
    Button4: TButton;
    Btn55: TButton;
    Btn65: TButton;
    Btnterceiros: TButton;
